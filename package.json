{
  "name": "@shoginn/discordjs-mock",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": "@ShoGinn",
  "license": "MIT",
  "files": ["dist"],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    }
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^18.18.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup index.ts --target node18 --format cjs,esm --dts --sourcemap",
    "build-watch": "pnpm build -- --watch",
    "clean": "rm -rf dist coverage",
    "test": "pnpm jest --coverage",
    "test:watch": "pnpm jest --watch",
    "format": "prettier --ignore-path .gitignore --config .prettierrc.cjs --write .",
    "format:check": "prettier --ignore-path .gitignore --config .prettierrc.cjs --check .",
    "lint": "eslint . && tsc --noEmit",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "discord.js": "^14.13.0"
  },
  "dependencies": {
    "@sapphire/snowflake": "^3.5.1",
    "discord.js": "^14.13.0",
    "lodash": "^4.17.21"
  }
}
